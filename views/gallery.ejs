<div class="profile-body">
	<div class="profile-wraper">
		<div class="profile-info">
			<div class="user info"><%- include ("layouts/profile.ejs") %></div>
			<hr />
			<div class="user-menu"><%- include ("layouts/menu.ejs") %></div>
		</div>
		<div class="profile-box">
			<div class="box-content">
				<h3>Gallery Photo</h3>
			</div>
			<div class="gall-update">
				<div class="gallery-wrapper">
					<div class="profile-img">
						<img
							src="https://www.finetoshine.com/wp-content/uploads/2020/04/Beautiful-Girl-Wallpapers-New-Photos-Images-Pictures.jpg"
							style="height: 100px"
						/>
					</div>
					<div class="profile-img">
						<img
							src="https://www.finetoshine.com/wp-content/uploads/2020/04/Beautiful-Girl-Wallpapers-New-Photos-Images-Pictures.jpg"
							style="height: 100px"
						/>
					</div>
					<div class="profile-img">
						<img
							src="https://www.finetoshine.com/wp-content/uploads/2020/04/Beautiful-Girl-Wallpapers-New-Photos-Images-Pictures.jpg"
							style="height: 100px"
						/>
					</div>
					<div class="profile-img">
						<img
							src="https://www.finetoshine.com/wp-content/uploads/2020/04/Beautiful-Girl-Wallpapers-New-Photos-Images-Pictures.jpg"
							style="height: 100px"
						/>
					</div>
				</div>
				<form action="/gallery" method="POST" enctype="multipart/form-data">
					<div class="form-group">
						<label for="gallery"
							><img id="label_img" src="../images/image-gallery.png" alt=""
						/></label>
						<input type="file" multiple name="gallery" id="gallery" />
						<button type="submit">Update</button>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>
